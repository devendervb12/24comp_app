<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="view.Page1" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Page 1">
		<content>
		      <List items="{/ProductSet}">
		         <items>
		           <StandardListItem title="{ProductID}"/>
		         </items>
		      </List>
	       
		</content>
		<footer>
		   <Bar>
		      <contentRight>
		                <Button text="Go to Page 2" press="gotoPage2"/>
		      </contentRight>
		   </Bar>
		</footer>
	</Page>
</core:View>